{
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "files.exclude": {
    "**/.git": false
  },
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "git.autofetch": true,
  "editor.fontSize": 14,
  "window.zoomLevel": 3,
  "markdown-pdf.convertOnSave": false,
  "markdown-pdf.styles": ["pdf-styles.css"],
  "markdown-pdf.margin.top": "1.5cm",
  "markdown-pdf.margin.bottom": "1cm",
  "markdown-pdf.margin.right": "2cm",
  "markdown-pdf.margin.left": "1cm",
  "markdown-pdf.outputDirectory": "./pdfs",
  "markdown-pdf.headerTemplate": "<div style=\"font-size: 8px; margin-left: 1cm; font-family: 'Segoe WPC', 'Segoe UI', 'SFUIText-Light', 'HelveticaNeue-Light', sans-serif, 'Droid Sans Fallback'; \">Clarx - <span class='title'></span></div> <div style=\"font-size: 8px; margin-left: auto; margin-right: 1cm; font-family: 'Segoe WPC', 'Segoe UI', 'SFUIText-Light', 'HelveticaNeue-Light', sans-serif, 'Droid Sans Fallback'; \"> <span>v1.0.0, last reviewed <span class='date'></span></span></div>",
  "markdown-pdf.footerTemplate": "<div style=\"font-size: 8px; margin-left: 1cm; font-family: 'Segoe WPC', 'Segoe UI', 'SFUIText-Light', 'HelveticaNeue-Light', sans-serif, 'Droid Sans Fallback';\">Clarx &copy; 2020</div><div style=\"font-size: 8px; margin-left: auto; margin-right: 1cm; font-family: 'Segoe WPC', 'Segoe UI', 'SFUIText-Light', 'HelveticaNeue-Light', sans-serif, 'Droid Sans Fallback';\"> <span class='pageNumber'></span> of <span class='totalPages'></span><img class='coder-square' src='./branding/CA_square.png' width='40' /></div>",
  "explorer.confirmDelete": false,
  "markdown-toc.anchorMode": "github.com",
  "markdown-toc.depthFrom": 1,
  "markdown-toc.depthTo": 3,
  "markdown-preview-enhanced.scrollSync": true,
  "markdown.extension.orderedList.autoRenumber": false,
  "editor.renderWhitespace": "boundary",
  "markdown.extension.toc.githubCompatibility": true,
  "markdown.extension.toc.levels": "1..3",
  "markdown-preview-enhanced.previewTheme": "github-dark.css",
  "markdown.extension.toc.updateOnSave": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontWeight": "600",
  "workbench.colorTheme": "Dark+ (contrast)",
  "git.confirmSync": false,
  "breadcrumbs.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.fontAliasing": "auto",
  "explorer.confirmDragAndDrop": false,
  "markdown-pdf.breaks": true,
  "markdown-pdf.orientation": "portrait",
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.dragAndDrop": false,
  "editor.find.autoFindInSelection": true,
  "material-icon-theme.folders.color": "#90a4ae",
  "material-icon-theme.hidesExplorerArrows": true,
  "python.jediEnabled": false,
  "todo.embedded.providers.ag.regex": "(?:#|//|/\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(TODO|FIXME|FIX|BUG|UGLY|HACK|NOTE|IDEA|OPTIMIZE)",
  "todo.embedded.providers.rg.regex": "(?:#|//|/\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(TODO|FIXME|FIX|BUG|UGLY|HACK|NOTE|IDEA|OPTIMIZE)",
  "todo.embedded.regex": "(?:<!-- *)?(?:#|//|/\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(TODO|FIXME|FIX|BUG|UGLY|HACK|NOTE|IDEA|OPTIMIZE)(?:\\s*\\([^)]+\\))?:?(?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|((?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))",
  "todo.colors.comment": "#EEEEEE",
  "typescript.locale": "en",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.wordWrap": "on",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#2979FF",
    "list.activeSelectionForeground": "#2979FF",
    "list.inactiveSelectionForeground": "#2979FF",
    "list.highlightForeground": "#2979FF",
    "scrollbarSlider.activeBackground": "#2979FF50",
    "editorSuggestWidget.highlightForeground": "#2979FF",
    "textLink.foreground": "#2979FF",
    "progressBar.background": "#2979FF",
    "pickerGroup.foreground": "#2979FF",
    "tab.activeBorder": "#2979FF",
    "notificationLink.foreground": "#2979FF",
    "editorWidget.resizeBorder": "#2979FF",
    "editorWidget.border": "#2979FF",
    "settings.modifiedItemIndicator": "#2979FF",
    "settings.headerForeground": "#2979FF",
    "panelTitle.activeBorder": "#2979FF",
    "breadcrumb.activeSelectionForeground": "#2979FF",
    "menu.selectionForeground": "#2979FF",
    "menubar.selectionForeground": "#2979FF"
  },
  "materialTheme.accent": "Blue",
  "workbench.editor.swipeToNavigate": true,
  "git.enableSmartCommit": true,
  "prettier.arrowParens": "always",
  "prettier.singleQuote": true,
  "prettier.tabWidth": 4,
  "cSpell.userWords": [],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}

